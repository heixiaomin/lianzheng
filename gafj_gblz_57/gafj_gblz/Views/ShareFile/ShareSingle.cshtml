@{
    ViewBag.Title = "ShareSingle";
    Layout = "~/Views/_PartialPage2.cshtml";
}



<style>
    #row1
    {
        margin-top: -2px;
    }

    #row2
    {
        margin-top: -2px;
    }
</style>

<div class="row" id="row1">
    <div class="col-md-12">
        <!--breadcrumbs start -->
        <ul class="breadcrumb panel">
            <li><a href="#"><i class="fa fa-home"></i>首页</a></li>
            <li><a href="#">档案审核</a></li>
           @* <li><a href="#">党工委、管委会办公室（信访局）</a></li>
            <li><a href="#">综合科</a></li>*@
            <li class="active">廉政档案登记表</li>
        </ul>
        <!--breadcrumbs end -->
    </div>
</div>

<div class="row" id="row2">
    <div class="col-sm-12">

        @{  
             
            var ulist = ViewBag.ulist as List<Yisaa.DAL.DT_UserLoginRecord>;
            foreach (var total in (ViewBag.tlist as List<Yisaa.DAL.DT_Total>))
            {
                foreach (var ulogin in ulist.Where(a => a.Id == total.UserId))
                {                  
            <section class="panel">
                <input type="text" value="@total.Id" hidden="hidden" id="myid"/>
                <header class="panel-heading">【@ulogin.RealName】廉政档案登记表
                    <div class="fr"><input type="checkbox" id="a_@ulogin.Id" value="@ulogin.RealName" onclick="dblist(1, event)">
                      <label for="a_@ulogin.Id">设置共享</label></div>
                </header>
                <div class="panel-body">
                    <div class="alert alert-block alert-danger fade in">
                        <button type="button" class="close close-sm" data-dismiss="alert"><i class="fa fa-times"></i></button>
                        <strong>提示：</strong>本年度档案登记表填写时间2017年6月1日至2017年7月1日，请在有效期内填写完毕并提交归档！
                    </div>
                    <table class="table  table-hover general-table">
                        <thead>
                            <tr>
                                <th>序号</th>
                                <th class="hidden-phone">表单名称</th>
                                <th width="10%">操作</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>1 </td>
                                <td class="hidden-phone">
                                    <a href="#">科级干部基本情况登记表一</a>

                                </td>
                                <td>

                                    <a class="btn btn-success btn-xs" href="#">查看</a>


                                </td>
                            </tr>
                            <tr>
                                <td>2 </td>
                                <td class="hidden-phone"><a href="#">科级干部奖惩情况登记表</a>

                                </td>
                                <td>
                                    <a class="btn btn-success btn-xs" href="#">查看</a>
                                </td>
                            </tr>
                            <tr>
                                <td>3 </td>
                                <td class="hidden-phone">
                                    <a href="#">科级干部拒收或上交礼金、礼品情况登记表</a>

                                </td>

                                <td>
                                    <a class="btn btn-success btn-xs" href="#">查看</a>
                                </td>
                            </tr>
                            <tr>
                                <td>4 </td>
                                <td class="hidden-phone">
                                    <a href="#">科级干部从事或参与营业性活动及兼职持股情况报告表</a>
                                </td>
                                <td>
                                    <a class="btn btn-success btn-xs" href="#">查看</a>

                                </td>
                            </tr>

                            <tr>
                                <td>5 </td>
                                <td class="hidden-phone">
                                    <a href="#">科级干部婚姻变化情况报告表</a>

                                </td>
                                <td>
                                    <a class="btn btn-success btn-xs" href="#">查看</a>

                                </td>
                            </tr>
                        </tbody>

                    </table>

                    <p class="t_center">


                        <button class="btn btn-primary btn-lg" type="button" disabled="disabled" id="handin_checking">已归档~</button>



                    </p>
                </div>
            </section>
                }
            }}

    </div>
</div>



<div id="xsyc"></div>
<div id="bbv">
  <div id="dbimg" onClick="dbhidd()"></div>
  <div id="dbnr">
    <h4><span>共享档案</span><span class="span2" onClick="dbhidd()"><img src="~/Content/images/closex.gif" align="absmiddle" />&nbsp;隐藏</span></h4>
    <div id="tishi">对不起，您还没有选择要共享的档案</div>
    <ul>
    </ul>
    <div class="pkbut"> <span><a href="/ShareFile/SetShare" onClick="">设置共享</a></span> <span onClick="clears()">清&nbsp;空</span> </div>
  </div>
</div>
 
