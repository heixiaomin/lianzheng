<script>
    $(function () {
        //提交归档
        $("#submit_check").click(function () {

            $.post("/CheckFile/_UpdTotal_Submit_High?id=" + $("#myid").val(),
             {
                 "id": $("#myid").val(),
             },
             function (data) {
                 if (data == "1") {
                     alert("提交成功！");

                     location.reload();

                 } else {

                     alert("提交失败,请重试！");

                 }
             });
        });
        //退回修改
        $("#back_upd").click(function () {

            $.post("/CheckFile/_UpdTotal_BackUpd_High?id=" + $("#myid").val(),
             {
                 "id": $("#myid").val(),
             },
             function (data) {
                 if (data == "1") {
                     alert("退回成功！");

                     location.reload();

                 } else {

                     alert("退回失败,请重试！");

                 }
             });
        });


    });


</script>
