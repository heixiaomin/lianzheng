@{
    ViewBag.Title = "UpdIndustryChange";
    Layout = "~/Views/_LayoutPage1.cshtml";
}
 
<div class="row">
    <div class="col-md-12">
        <!--breadcrumbs start -->
        <ul class="breadcrumb panel">
            <li><a href="/EnterFile/Index"><i class="fa fa-home"></i>首页</a></li>
            <li class="active">科级干部配偶、子女及其配偶从业变更情况报告表</li>
        </ul>
        <!--breadcrumbs end -->
    </div>
</div>
   <div class="row">
			
                <div class="col-sm-12">
                    <section class="panel">
                        <header class="panel-heading">
                          【@Session["realname"]】配偶、子女及其配偶从业变更情况报告表</header>
						  <div class="panel-body">
						<div class="alert alert-block alert-danger fade in">
                <button type="button" class="close close-sm" data-dismiss="alert"> <i class="fa fa-times"></i> </button>
                <strong>说明：</strong>填写从2010年1月以后发生变更的情况。</div>

                              @{
                                  var industry = ViewBag.industry as List<Yisaa.DAL.DT_IndustryChanges>;
                                  foreach (var i in industry)
                                  {
                               
                                        @if (ViewBag.status == "被退回")
                                        {
                        <form class="form-horizontal" role="form" action="/EnterFile/_UpdIndustryChange_Back" method="post">
						<h5 style="background:#eff2f7; font-weight:bold; color:#65cea7; line-height:30px; padding-left:20px; border-radius:4px;">配偶、子女及其配偶从业变更情况</h5>
                            <div class="form-group">
                                <label for="inputEmail1" class="col-lg-3 col-sm-2 control-label">从业变更人员</label>
                                <div class="col-lg-4">
                                    <input type="text" class="form-control" id="inputEmail1" placeholder="" name="name" value="@i.Name">
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="input1" class="col-lg-3 col-sm-2 control-label">与填表人关系</label>
                                <div class="col-lg-4">
                                    <input type="" class="form-control" id="input1" placeholder="" name="relation" value="@i.Relation">
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
                            </div>
							    <div class="form-group">
                                <label for="input1" class="col-lg-3 col-sm-2 control-label">从业变更时间</label>
                                <div class="col-lg-4">
                                    <input type="" class="form-control laydate-icon" id="time" placeholder="" name="time" value="@i.ChangeTime">
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
                            </div>
							
							
							    <div class="form-group">
                                <label for="input1" class="col-lg-3 col-sm-2 control-label">变更前工作单位及职务</label>
                                <div class="col-lg-4">
                                    <input type="" class="form-control" id="input1" placeholder="" name="before_unit" value="@i.LastUnit">
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
								
                            </div>
							    <div class="form-group">
                                <label for="input1" class="col-lg-3 col-sm-2 control-label">变更后工作单位及职务</label>
                                <div class="col-lg-4">
                                    <input type="" class="form-control" id="input1" placeholder="" name="behind_unit" value="@i.NowUnit">
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
                            </div>
							 <div class="form-group">
                                <label for="input1" class="col-lg-3 col-sm-2 control-label">变更原因</label>
                                <div class="col-lg-4">
                                    <input type="" class="form-control" id="input1" placeholder="" name="reason" value="@i.Reason">
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
                            </div>
								<div class="form-group">
                                <label for="input1" class="col-lg-3 col-sm-2 control-label">备注</label>
                                <div class="col-lg-4">
                                   <textarea rows="6" class="form-control" name="remark">@i.Remark</textarea>
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
								 </div>
							
                            <div class="form-group">
                                <div class="col-lg-offset-3 col-lg-10">
                                    
                                   
                                    <button type="submit" class="btn btn-primary">确认修改</button>
                                    
									<button class="btn btn-default" type="button" onclick="window.history.go(-1);">返回</button>
                                </div>
                            </div>
                        </form>
                                            
                                        }
                                        else
                                        {
                                              <form class="form-horizontal" role="form" action="/EnterFile/_UpdIndustryChange" method="post">
						<h5 style="background:#eff2f7; font-weight:bold; color:#65cea7; line-height:30px; padding-left:20px; border-radius:4px;">配偶、子女及其配偶从业变更情况</h5>
                            <div class="form-group">
                                <label for="inputEmail1" class="col-lg-3 col-sm-2 control-label">从业变更人员</label>
                                <div class="col-lg-4">
                                    <input type="text" class="form-control" id="inputEmail1" placeholder="" name="name" value="@i.Name">
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="input1" class="col-lg-3 col-sm-2 control-label">与填表人关系</label>
                                <div class="col-lg-4">
                                    <input type="" class="form-control" id="input1" placeholder="" name="relation" value="@i.Relation">
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
                            </div>
							    <div class="form-group">
                                <label for="input1" class="col-lg-3 col-sm-2 control-label">从业变更时间</label>
                                <div class="col-lg-4">
                                    <input type="" class="form-control laydate-icon" id="time" placeholder="" name="time" value="@i.ChangeTime">
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
                            </div>
							
							
							    <div class="form-group">
                                <label for="input1" class="col-lg-3 col-sm-2 control-label">变更前工作单位及职务</label>
                                <div class="col-lg-4">
                                    <input type="" class="form-control" id="input1" placeholder="" name="before_unit" value="@i.LastUnit">
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
								
                            </div>
							    <div class="form-group">
                                <label for="input1" class="col-lg-3 col-sm-2 control-label">变更后工作单位及职务</label>
                                <div class="col-lg-4">
                                    <input type="" class="form-control" id="input1" placeholder="" name="behind_unit" value="@i.NowUnit">
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
                            </div>
							 <div class="form-group">
                                <label for="input1" class="col-lg-3 col-sm-2 control-label">变更原因</label>
                                <div class="col-lg-4">
                                    <input type="" class="form-control" id="input1" placeholder="" name="reason" value="@i.Reason">
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
                            </div>
								<div class="form-group">
                                <label for="input1" class="col-lg-3 col-sm-2 control-label">备注</label>
                                <div class="col-lg-4">
                                   <textarea rows="6" class="form-control" name="remark">@i.Remark</textarea>
                                    <!--<p class="help-block">Example block-level help text here.</p>-->
                                </div>
								 </div>
							
                            <div class="form-group">
                                <div class="col-lg-offset-3 col-lg-10">
                                    
                                   
                                    <button type="submit" class="btn btn-primary">确认修改</button>
                                    
									<button class="btn btn-default" type="button" onclick="window.history.go(-1);">返回</button>
                                </div>
                            </div>
                        </form>
                                        }
                                  }
                               
                                  }
                    </div>
                    </section>
					
                </div>
            </div>


<script src="~/Content/js/jquery-1.10.2.min.js"></script>

<script src="~/Content/myjs/datepicker/WdatePicker.js"></script>
<script src="~/Content/myjs/datepicker/calendar.js"></script>

